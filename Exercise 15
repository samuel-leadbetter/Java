// 15. Write a Java program to swap two variables.

public class Main {

    public static void main(String[] args) {
        int temp = 0;
        int var1 = 3;
        int var2 = 5;
        System.out.println("1st variable = " + var1);
        System.out.println("2nd variable = " + var2);
        System.out.println("Swapping variables...");
        temp = var1;
        var1 = var2;
        var2 = temp;
        System.out.println("1st variable = " + var1);
        System.out.println("2nd variable = " + var2);
    }


}
