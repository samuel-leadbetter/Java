// 16. Write a Java program to print a face.

 public class Main {

    public static void main(String[] args) {
        System.out.println("  ^^^^^^^  ");
        System.out.println("{| O   O |}");
        System.out.println(" |   L   | ");
        System.out.println(" |   -   | ");
        System.out.println("   |   |   ");
    }
}
